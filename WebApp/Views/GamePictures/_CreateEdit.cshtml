@model WebApp.ViewModels.GamePictures.GamePictureCreateEditViewModel

<div class="form-group">
    <label asp-for="GamePicture.PictureUrl" class="control-label"></label>
    <input asp-for="GamePicture.PictureUrl" class="form-control" />
    <span asp-validation-for="GamePicture.PictureUrl" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="GamePicture.DateAdded" class="control-label"></label>
    <input asp-for="GamePicture.DateAdded" class="form-control" />
    <span asp-validation-for="GamePicture.DateAdded" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="GamePicture.GameInfoId" class="control-label"></label>
    <select asp-for="GamePicture.GameInfoId" class ="form-control" asp-items="Model.GameInfoSelectList"></select>
</div>