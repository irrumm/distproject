@model WebApp.ViewModels.GameAwards.GameAwardCreateEditViewModel

<div class="form-group">
    <label asp-for="GameAward.YearWon" class="control-label"></label>
    <input asp-for="GameAward.YearWon" class="form-control" />
    <span asp-validation-for="GameAward.YearWon" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="GameAward.GameInfoId" class="control-label"></label>
    <select asp-for="GameAward.GameInfoId" class ="form-control" asp-items="Model.GameInfoSelectList"></select>
</div>
<div class="form-group">
    <label asp-for="GameAward.AwardId" class="control-label"></label>
    <select asp-for="GameAward.AwardId" class ="form-control" asp-items="Model.AwardSelectList"></select>
</div>